
/**
 *  Write the minimal Huff code for a contract that returns 
 *  the current block number if called
*/

#define macro MAIN() = takes(0) returns(0) {
    //get block number for current block
    number        //[number]
    0x00          //[0, number]
    mstore        //[]

    //return number
    0x20          //[32]
    0x00          //[0,32]
    return
}